{
  "name": "smplphp/collections",
  "description": "A simple, modern PHP 8 collections library, providing a number of data-structures all wrapped around arrays",
  "minimum-stability": "dev",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Ollie Read",
      "email": "smpl@ollie.codes"
    }
  ],
  "require": {
    "php": "^8.1",
    "smplphp/utilities": "dev-develop"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "sempro/phpunit-pretty-print": "^1.4",
    "vimeo/psalm": "^5.0",
    "infection/infection": "^0.26.9",
    "smplphp/composer-local-package-repository": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "Smpl\\Collections\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smpl\\Collections\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true,
      "smplphp/composer-local-package-repository": true
    }
  }
}
